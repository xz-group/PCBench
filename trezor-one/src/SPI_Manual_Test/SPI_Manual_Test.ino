  digitalWrite(DC, LOW);
  digitalWrite(SS, LOW); 
  for(int i=0; i < sizeof(startup)/sizeof(byte); i++) {
      //Serial.println(startup[i]);
      SPI.transfer(startup[i]);
  }
  digitalWrite(SS, HIGH);

  digitalWrite(SS, LOW); 
  for(int i=0; i < sizeof(preData)/sizeof(byte); i++) {
      SPI.transfer(preData[i]);
  }
  digitalWrite(SS, HIGH);

  //start blank
  digitalWrite(SS, LOW); 
  digitalWrite(DC, HIGH);
  for(int i=0; i < 1024; i++) {
    SPI.transfer(0x00);
  }
  digitalWrite(DC, LOW);
  digitalWrite(SS, HIGH); 


